[
  {  
    "context": "Workspace",
    "bindings": {
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "cmd-=": "pane::SplitRight",
      "cmd-;": "pane::SplitDown",
      "alt-shift-h": "workspace::ActivateNextPane",
      "alt-shift-l": "workspace::ActivatePreviousPane",
      "alt-shift-j": "workspace::ActivatePaneDown",
      "alt-shift-k": "workspace::ActivatePaneUp",
      "cmd-shift-h": "pane::ActivatePreviousItem",
      "cmd-shift-l": "pane::ActivateNextItem",
      "cmd-shift-j": "pane::SwapItemLeft",
      "cmd-shift-k": "pane::SwapItemRight",
      "cmd-shift-c": "pane::CloseActiveItem"
    }
  },
  {
    "context": "Dock",
    "bindings": {
      "cmd-=": "pane::SplitRight",
      "cmd-;": "pane::SplitDown",
      "alt-shift-h": "workspace::ActivateNextPane",
      "alt-shift-l": "workspace::ActivatePreviousPane",
      "alt-shift-j": "workspace::ActivatePaneDown",
      "alt-shift-k": "workspace::ActivatePaneUp"
    }
  }
]
