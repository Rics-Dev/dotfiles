[
  {
    "context": "Workspace",
    "bindings": {
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "cmd-n": "workspace::NewTerminal",
      "ctrl-w v": "pane::SplitRight",
      "ctrl-w s": "pane::SplitDown",
      "cmd-shift-h": "workspace::ActivateNextPane",
      "cmd-shift-l": "workspace::ActivatePreviousPane",
      "cmd-shift-j": "workspace::ActivatePaneDown",
      "cmd-shift-k": "workspace::ActivatePaneUp",
      "shift-h": "pane::ActivatePreviousItem",
      "shift-l": "pane::ActivateNextItem",
      "shift-j": "pane::SwapItemLeft",
      "shift-k": "pane::SwapItemRight",
      "shift-q": "pane::CloseActiveItem"
    }
  },
  {
    "context": "Editor && vim_mode == insert",
    "bindings": {
      "escape": ["workspace::SendKeystrokes", "cmd-s escape"]
    }
  },
]
